{
  "name": "aaid",
  "version": "0.1.3",
  "description": "AAID: reusable prompts and rules with a typed, extendable API.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./prompts": {
      "types": "./dist/prompts/index.d.ts",
      "import": "./dist/prompts/index.js",
      "require": "./dist/prompts/index.cjs"
    },
    "./rules": {
      "types": "./dist/rules/index.d.ts",
      "import": "./dist/rules/index.js",
      "require": "./dist/rules/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup && tsc -p tsconfig.json --emitDeclarationOnly",
    "verify": "npm run build && npm pack --dry-run",
    "release:patch": "npm version patch && npm publish --access public",
    "release:minor": "npm version minor && npm publish --access public",
    "release:major": "npm version major && npm publish --access public",
    "release:patch:nogit": "npm version patch --no-git-tag-version && npm publish --access public",
    "release:minor:nogit": "npm version minor --no-git-tag-version && npm publish --access public",
    "release:major:nogit": "npm version major --no-git-tag-version && npm publish --access public",
    "prepare": "npm run build"
  },
  "keywords": [
    "aaid",
    "prompts",
    "rules",
    "ai",
    "typescript"
  ],
  "author": "Dawid Dahl",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dawid-dahl-umain/augmented-ai-development.git",
    "directory": "packages/aaid"
  },
  "bugs": {
    "url": "https://github.com/dawid-dahl-umain/augmented-ai-development/issues"
  },
  "homepage": "https://github.com/dawid-dahl-umain/augmented-ai-development#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
